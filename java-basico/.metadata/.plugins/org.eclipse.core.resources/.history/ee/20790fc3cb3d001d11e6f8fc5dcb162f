
public class TestaSomatoria {

	public static void main(String[] args) {
		
		int contador = 0; // Declaração e inicialização de variável.
		int total = 0; // Declaração e inicialização de variável.
		
		while(contador <= 10) { // 
			
			total += contador;
			
			System.out.println(total);
			
		   contador++;
		}
		
	}
}
